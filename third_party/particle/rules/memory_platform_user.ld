/*
 * Memory platform user configuration for Particle P2
 * This file is normally generated by the Particle build system.
 * For our Bazel build, we use default values for the P2 platform.
 */

/* User part flash size - default for P2 is 1536K */
platform_user_part_flash_size = 256K;

/* User part RAM sizes */
platform_user_part_static_ram_size = 60K;
platform_user_part_secure_ram_size = 0;
platform_user_part_psram_size = 64K;
